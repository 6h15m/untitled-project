    "build:back:w": "tsc -w -p back/tsconfig.json",
    "build:models:w": "tsc -w -p models/tsconfig.json",
    "webpack:front:w": "webpack --watch --config ./front/webpack.config.cjs",
    "build-webpack:front:src:w": "run-p build:front:src:w webpack:front:w",
    "build-webpack:front:w": "run-p build:front:server:w build-webpack:front:src:w",
    "nodemon:front:w": "nodemon --watch ./dist/front/public/ ./dist/front/server-dist/app.js",
    "nodemon:back:w": "nodemon --watch ./dist/back/ ./dist/back/index.js",
    "start:back:w": "run-p build:back:w nodemon:back:w",
    "start:front:w": "run-p build-webpack:front:w nodemon:front:w"
